{
  "allowed_domains": [
    "video.twimg.com",
    "pbs.twimg.com"
  ],
  "blocked_domains": [],
  "rate_limit_per_minute": 60,
  "custom_headers": {
    "X-Proxy-By": "EmbedProxy/1.0"
  },
  "strip_tracking_headers": true,
  "user_agent": "curl/8.15.0",
  "upstream_timeout": 30,
  "connection_timeout": 10,
  "read_timeout": 30,
  "write_timeout": 30,
  "cache": {
    "enabled": true,
    "max_items": 10000,
    "default_ttl_seconds": 3600,
    "max_ttl_seconds": 86400,
    "max_item_size_mb": 10,
    "cacheable_status_codes": [200, 203, 300, 301, 302, 404, 410],
    "cacheable_content_types": [
      "text/html",
      "text/css", 
      "text/javascript",
      "application/javascript",
      "application/json",
      "image/jpeg",
      "image/png",
      "image/gif", 
      "image/webp",
      "image/svg+xml",
      "video/mp4"
    ],
    "no_cache_domains": [
      "api.twitter.com",
      "graph.facebook.com"
    ],
    "enable_compression": true
  },
  "enable_metrics": true
}
